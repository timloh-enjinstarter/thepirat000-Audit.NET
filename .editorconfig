# http://editorconfig.org
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{json,yml,csproj,props}]
indent_size = 2

[.vscode/**.json]
indent_size = 4

[.devcontainer/devcontainer.json]
indent_size = 4
